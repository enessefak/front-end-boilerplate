{
    "root": true,
    "extends": ["prettier"],
    "plugins": ["prettier", "import"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error",
      "semi": "off",
      "comma-dangle": "off"
    }
  }
  