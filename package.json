{
  "name": "front-end-boilerplate",
  "version": "1.0.0",
  "description": "Modern Front-end Boilerplate (Gulp, Webpack)",
  "scripts": {
    "start": "gulp --require @babel/register --gulpfile tasks",
    "build": "cross-env NODE_ENV=production gulp build --require babel-register --gulpfile tasks",
    "build:js": "cross-env NODE_ENV=production gulp scripts --require babel-register --gulpfile tasks",
    "build:css": "cross-env NODE_ENV=production gulp css --require babel-register --gulpfile tasks",
    "build:img": "cross-env NODE_ENV=production gulp images --require babel-register --gulpfile tasks",
    "build:html": "cross-env NODE_ENV=production gulp html --require babel-register --gulpfile tasks"
  },
  "author": {
    "name": "Enes Sefa Küccük",
    "email": "enes.sefa.k@gmail.com",
    "url": "https://github.com/enessefak/front-end-boilerplate"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-object-assign": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "autoprefixer": "^10.3.1",
    "browser-sync": "^2.27.4",
    "critical": "^4.0.0",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "prettier": "^2.3.2",
    "pug": "^3.0.2",
    "sass": "^1.35.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "sw-precache": "^5.2.1",
    "webpack": "^5.44.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "core-js": "^3.15.2",
    "flexiblegs-scss": "^5.6.0",
    "jquery": "^3.6.0",
    "normalize-css": "^2.3.1",
    "slick-carousel": "^1.8.1"
  }
}
